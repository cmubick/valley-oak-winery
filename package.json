{
  "name": "winery-website",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "localstack:start": "docker-compose up -d",
    "localstack:stop": "docker-compose down",
    "localstack:setup": "./scripts/setup-localstack.sh",
    "dev:local": "npm run localstack:start && sleep 5 && npm run localstack:setup && npm run dev",
    "lambda:build": "./scripts/build-lambda.sh",
    "lambda:deploy": "npm run lambda:build && cd terraform && terraform apply",
    "tf:init": "cd terraform && terraform init",
    "tf:plan": "cd terraform && terraform plan",
    "tf:apply": "cd terraform && terraform apply"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.840.0",
